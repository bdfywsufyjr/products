- var domain = 'http://'+env.APP_DOMAIN+'/';
doctype html
html
  head
    title= title

    <!-- Main CSS-->
    link(rel='stylesheet', href='https://maxcdn.bootstrapcdn.com/bootstrap/4.1.0/css/bootstrap.min.css')
    link(rel='stylesheet', href='/stylesheets/style.css')

    script(src='https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js')
    script(src='https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.0/umd/popper.min.js')
    script(src='https://maxcdn.bootstrapcdn.com/bootstrap/4.1.0/js/bootstrap.min.js')

    <!-- Font-icon css-->
    link(rel='stylesheet' type='text/css' href='https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css')

    <!-- jQuery-->
    script(src='https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js')

    <!-- Datatables extension-->
    link(data-require='datatables@*' data-semver='1.10.12' rel='stylesheet' href='https://cdn.datatables.net/1.10.12/css/jquery.dataTables.min.css')
    link(rel='stylesheet' href='https://cdn.datatables.net/rowreorder/1.2.0/css/rowReorder.dataTables.min.css')
    link(rel='stylesheet' href='https://cdn.datatables.net/buttons/1.0.3/css/buttons.dataTables.min.css')

    script(data-require='datatables@*' data-semver='1.10.19' src='https://cdn.datatables.net/1.10.19/js/jquery.dataTables.min.js')
    script(src='https://cdn.datatables.net/buttons/1.0.3/js/dataTables.buttons.min.js' type='text/javascript')
    script(src='https://cdn.datatables.net/buttons/1.0.3/js/buttons.html5.min.js' type='text/javascript')
    script(src='https://cdn.datatables.net/buttons/1.0.3/js/buttons.print.min.js' type='text/javascript')
    script(src='https://cdn.datatables.net/buttons/1.0.3/js/buttons.flash.min.js' type='text/javascript')
    script(src='https://cdnjs.cloudflare.com/ajax/libs/jszip/2.5.0/jszip.min.js' type='text/javascript')
    script(src='https://cdn.rawgit.com/bpampuch/pdfmake/0.1.18/build/pdfmake.min.js' type='text/javascript')
    script(src='https://cdn.rawgit.com/bpampuch/pdfmake/0.1.18/build/vfs_fonts.js' type='text/javascript')

  body

    <!-- Navbar-->

    nav(class='navbar navbar-expand-sm bg-dark navbar-dark container')
      a(class='navbar-brand' href= domain)
        img(src= domain + 'images/logo.jpg' alt='Logo' style='width:100px;')
      ul(class='navbar-nav')
        li(class='nav-item')
          a(class='nav-link' href= domain) Главная
        li(class='nav-item')
          a(class='nav-link' href= domain + 'data/settings') Настройки

    <!-- App content-->

    main(class='app-content')
      div(class='container')
        div(class="row")
          div(class="col")

            block content


            script.
                $(document).ready(function () {
                    $('#pickle').DataTable({});
                });



